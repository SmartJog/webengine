[uwsgi]
plugins = python3
uid = sjuser
gid = sjgroup
umask = 002
socket = /run/uwsgi/app/webengine/socket
chown-socket = sjuser:sjgroup
chdir = /usr/share/webengine/app
pythonpath = /usr/share/webengine/app
vacuum=True
wsgi-file= @webengineappdir@/webengine.wsgi
master= True
enable-threads = False
processes = 5
